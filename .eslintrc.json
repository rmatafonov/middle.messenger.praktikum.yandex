{
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "max-len": [
            2,
            100
        ],
        "@typescript-eslint/no-unused-vars": 2
    }
}
